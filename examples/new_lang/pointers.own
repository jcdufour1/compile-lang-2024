
extern("c") fn printf(format_string: ptr, args: any...) i32

fn add_one(num_ptr: i32*) {
    deref(num_ptr) = deref(num_ptr) + 1
    return
}

fn operation_2(num_ptr_2: i32*) {
    deref(num_ptr_2) = 2*deref(num_ptr_2) + 1
    return
}

fn main() i32 {
    let num: i32 = 8
    printf("%d\n", num)
    add_one(refer(num))
    printf("%d\n", num)
    operation_2(refer(num))
    printf("%d\n", num)
    return 0
}

