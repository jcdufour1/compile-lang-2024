
extern("c") fn printf(str_to_print u8*, args any...) i32

type Opt_int sum {
    some i32
    none void
}

fn opt_int_print(opt_num Opt_int) {
    switch opt_num {
        case Opt_int.some(num): printf("got number: %d\n", num)
        case Opt_int.none: printf("got nothing\n")
    }
}


fn main() i32 {
    let num Opt_int = Opt_int.some(76)
    opt_int_print(num)

    num = Opt_int.none
    opt_int_print(num)
    return 0
}
