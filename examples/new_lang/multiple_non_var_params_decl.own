

extern("c") fn fopen(path u8*, mode u8*) any*
extern("c") fn printf(format_string u8*, args any...) i32

fn main() i32 {
    let NULL_ANY any* = unsafe_cast<any*>0

    let file any* = fopen("examples/new_lang/multiple_non_var_params_decl.own", "r")
    if NULL_ANY == file {
        printf("error: could not open file\n")
        return 1
    }

    printf("success\n")

    return 0
}
