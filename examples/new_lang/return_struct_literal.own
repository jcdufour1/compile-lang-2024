
extern("c") fn printf(str_to_print u8*, args any...) i32

type Token struct {
    file u8*
    line i32
    str_data u8*
}

fn get_token() Token {
    return {.file = "file.txt", .line = 21, .str_data = "hello world"}
}

fn main() i32 {
    let token Token = get_token()
    printf("%s %d %s\n", token.file, token.line, token.str_data)

    return 0
}
