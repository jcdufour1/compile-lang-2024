io :: import = std.io
coll :: import = std.collections
util :: import = std.util

type Token struct {
    num i32
    string u8*
}

fn main() i32 {
    let arr coll.Arr(<Token, 1000000>) = coll.arr_new(<Token, 1000000>)()
    defer coll.arr_free(&arr);

    coll.arr_append(<Token>)(&arr, {9187, "hello"})
    io.printf("%s\n", coll.arr_first(<Token>)(arr).string);

    return 0
}
