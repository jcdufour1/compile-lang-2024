type io import = std.io
type coll import = std.collections
type util import = std.util

type Token struct {
    num i32
    string u8*
}

fn main() i32 {
    let arr coll.Arr(<Token>) = coll.arr_new(1000000, Token)
    defer coll.arr_free(&arr);

    coll.arr_append(&arr, {9187, "hello"}, Token)
    io.printf("%s\n", coll.arr_first(arr, Token).string);

    return 0
}
