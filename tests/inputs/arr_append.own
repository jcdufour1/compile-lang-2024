type io import = std.io
type coll import = std.collections
type util import = std.util

type Token struct {
    num i32
    string u8*
}

fn main() i32 {
    let arr coll.Arr(<Token>) = {unsafe_cast<Token*>util.malloc(1000000), 0, 100}
    util.assert(arr.buf != unsafe_cast<Token*>0, "out of memory")
    coll.arr_append(&arr, {9187, "hello"}, Token)
    io.printf("%s\n", coll.arr_first(arr, Token).string);
    return 0
}
