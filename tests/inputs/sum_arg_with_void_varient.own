

Token :: enum {
    open_par
    num i32
    string u8*
}

main :: fn() i32 {
    token1 Token := Token.open_par
    token2 Token := Token.open_par()
    token3 Token := Token.open_par(76)

    switch token1 {
        case .num(num): break
        case .open_par(): break
    }
    switch token1 {
        case .num(num): break
        case .open_par(num): break
    }
    switch token1 {
        case .num(num): break
        case .open_par(num, string): break
    }

    return 0
}
