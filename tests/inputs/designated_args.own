io :: import = std.io

print :: fn(number i32 = 43, string u8* = "hello") {
    io.printf("%d %s\n", number, string)
}

main :: fn() i32 {
    print()
    print(.number = 74)
    print(.number = 76, .string = "thing")
    print(.string = "thing")
    print(.string = "thing", .number = 76)
    print(76, .string = "thing")

    // TODO: change second arg below to positional arg if it becomes possible
    print(.number = 76, .string = "thing")
    
    return 0
}
