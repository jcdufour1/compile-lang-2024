type io import = std.io


fn for_range() {
    for idx i32 in 0..100 {
        io.printf("test 1: idx: %d\n", idx)

        if idx < 2 {
            continue
        }

        break
    }

    for idx i32 in 0..2 {
        io.printf("test 2: idx: %d\n", idx)
        continue
        io.printf("test 3: no\n")
    }

    label1: for idx i32 in 0..2 {
        io.printf("test 3.3: idx: %d\n", idx)
        continue ''label1
        io.printf("test 3.4: no\n")
    }

    for idx i32 in 0..2 {
        io.printf("test 4: idx: %d\n", idx)
        continue
    }

    for idx i32 in 0..2 {
        continue
    }
}

fn for_cond() {
    let idx1 i32 = 0
    for idx1 < 100 {
        io.printf("test 5: %d\n", idx1)

        idx1 = idx1 + 1

        if idx1 < 3 {
            continue
        }

        break
    }

    let idx2 i32 = 0
    for idx2 < 2 {
        io.printf("test 6: idx: %d\n", idx2)
        idx2 = idx2 + 1
        continue
        io.printf("test 7: no\n")
    }

    let idx3 i32 = 0
    for idx3 < 2 {
        io.printf("test 8: idx: %d\n", idx3)
        idx3 = idx3 + 1
        continue
    }
}

fn main() i32 {
    for_range()
    for_cond()

    return 0
}
