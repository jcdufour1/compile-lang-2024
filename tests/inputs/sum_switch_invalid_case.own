
extern("c") printf :: fn(str_to_print u8*, args opaque...) i32

Token :: enum {
    num i32
    string u8*
}

Thing :: enum {
    token Token
    num i32
}

token_print :: fn(token Token) {
    switch token {
        case Token.num(num): printf("token is number: %d\n", num)
        case Thing.token(token_type): printf("token is Thing.token")
    }
}

main :: fn() i32 {
    token_print(.num(78))
    return 0
}

