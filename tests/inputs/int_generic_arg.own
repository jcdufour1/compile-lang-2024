io :: import = std.io

Token :: enum(<max_cap ''i32>){
    num i32
}

print_max :: fn(<max_cap ''int>)(token Token(<max_cap>)) {
    io.print_int(max_cap)
}

main :: fn() i32 {
    token Token(<8>) := .num(7)
    print_max(<8>)(token)
    print_max(token)
    io.print_int(token.max_cap)

    return 0
}
