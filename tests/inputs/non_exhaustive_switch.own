
extern("c") puts :: fn(str_to_print_2 u8*) 
extern("c") printf :: fn(str_to_print_2 u8*, args opaque...) i32

TokenType :: enum {
    number
    string
}

Token :: struct {
    kind TokenType
    number_data i32
    string_data u8*
}

print_token_switch :: fn(token Token) {
    switch token.kind {
    case TokenType.number: printf("%d\n", token.number_data)
    }
}

main :: fn() i32 {
    let token_type TokenType = TokenType.number
    printf("%d\n", token_type);

    token_type = TokenType.string
    printf("%d\n", token_type);

    {
        let token Token
        token.kind = TokenType.string
        token.number_data = 87
        token.string_data = "hello"
        print_token_switch(token)
    }
    return 0
}

