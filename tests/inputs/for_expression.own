io :: import = std.io

fn is_true() i32 {
    return 1
}

fn is_false() i32 {
    return 0
}

fn main() i32 {
    let should_loop i32 = 1
    for should_loop {
        let temp i32 = 79
        io.printf("yes1\n")
        io.printf("%d\n", temp)
        should_loop = 0
    }
    for should_loop {
        io.printf("no1\n")
        return 1
    }

    for 0 {
        io.printf("no2\n")
        return 1
    }
    for 1 {
        let temp i32 = 80
        io.printf("yes2\n")
        io.printf("%d\n", temp)
        break
    }

    for is_false() {
        io.printf("no3\n")
        return 1
    }
    for is_true() {
        let temp i32 = 81
        io.printf("yes3\n")
        io.printf("%d\n", temp)
        break
    }

    io.printf("done\n")
    return 0
}
