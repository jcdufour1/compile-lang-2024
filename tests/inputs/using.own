
io2 :: import = std.io
coll2 :: import = std.collections
using io2
using coll2

Token :: struct {
    num i32
    string Str
}

using_struct :: fn(token Token) {
    using token

    print_int(num)
    print_str(string)

    print_int(token.num)
    print_str(token.string)
}

using_struct_concise :: fn(using token Token) {
    print_int(num)
    print_str(string)

    print_int(token.num)
    print_str(token.string)
}

main :: fn() i32 {
    print_str("hello")
    let token Token = {8, "world"}
    using_struct(token)
    token = {65, "computer"}
    using_struct_concise(token)
    return 0
}
