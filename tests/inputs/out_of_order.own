io :: import = std.io


main :: fn() i32 {
    let token Token = get_token("file57.txt", 914, {.data = "new data", .count = 8})
    io.printf("%s %d %s\n", token.file, token.line, token.str_data.data)

    return 0
}

get_token :: fn(file u8*, line i32, data Str_view) Token {
    return {.file = file, .line = line, .str_data = data}
}


Str_view :: struct {
    data u8*
    count i64
}

Token :: struct {
    file u8*
    line i32
    str_data Str_view
}

