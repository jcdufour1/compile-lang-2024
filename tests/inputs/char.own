type io import = std.io
type util import = std.util
type coll import = std.collections

fn string_comparison() {
    let string u8* = "hello"

    if string[0] == 'h' {
        io.printf("yes1\n");
    } else {
        io.printf("no1\n");
    }

    if string[0] > 'a' {
        io.printf("yes2\n");
    } else {
        io.printf("no2\n");
    }

    if string[0] > 'z' {
        io.printf("no3\n");
    } else {
        io.printf("yes3\n");
    }

    if string[0] < 'a' {
        io.printf("no4\n");
    } else {
        io.printf("yes4\n");
    }

    if string[0] < 'z' {
        io.printf("yes5\n");
    } else {
        io.printf("no5\n");
    }
}

fn literal_comparison() {
    if 'h' == 'h' {
        io.printf("yes6\n");
    } else {
        io.printf("no6\n");
    }

    if 'h' > 'a' {
        io.printf("yes7\n");
    } else {
        io.printf("no7\n");
    }

    if 'h' > 'z' {
        io.printf("no8\n");
    } else {
        io.printf("yes8\n");
    }

    if 'h' < 'a' {
        io.printf("no9\n");
    } else {
        io.printf("yes9\n");
    }

    if 'h' < 'z' {
        io.printf("yes10\n");
    } else {
        io.printf("no10\n");
    }
}

fn newline() {
    let string u8* = "hello\n"
    if string[5] == '\n' {
        io.printf("yes11\n")
    } else {
        io.printf("no11\n")
    }
}

fn main() i32 {
    string_comparison()
    literal_comparison()
    newline()
    return 0
}

