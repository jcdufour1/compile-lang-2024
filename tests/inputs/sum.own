io :: import = std.io

Token :: enum {
    num i32
    string u8*
}

Thing :: enum {
    token Token
    num i32
}

token_print :: fn(token Token) {
    switch token {
        case Token.num(num): io.printf("token is number: %d\n", num)
        case Token.string(str): io.printf("token is string: %s\n", str)
    }
}

thing_print :: fn(thing Thing) {
    switch thing {
        case Thing.token(currToken): {
            io.printf("thing is token: ")
            token_print(currToken)
        }
        case Thing.num(num): io.printf("thing is number: %d\n", num)
    }
}

main :: fn() i32 {
    token1 Token := Token.num(23)
    token2 Token := Token.string("hello world")
    token3 Token := Token.num(57)
    token4 Token := Token.num(32)

    thing1 Thing := Thing.token(token1)
    thing2 Thing := Thing.token(token2)
    thing3 Thing := Thing.num(87)
    token_print(token1)
    token_print(token2)
    token_print(token3)
    token_print(token4)

    thing_print(thing1)
    thing_print(thing2)
    thing_print(thing3)

    //thing_print(thing2)

    thing_print(Thing.num(25))

    return 0
}
