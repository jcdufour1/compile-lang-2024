
Opt_int :: enum {
    some i32
    none i1
}

opt_int_print :: fn(num Opt_int) {
    switch num {
        case Opt_int.some(num): printf("got number: %d\n", num)
        case Opt_int.none(dummy): printf("got nothing\n")
    }
}

main :: fn() i32 {
    num Opt_int := Opt_int.some(76)
    opt_int_print(num)
    return 0
}
