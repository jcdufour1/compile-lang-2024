
coll :: import = std.collections
io :: import = std.io

Token :: struct {
    num i32
}

read :: fn(slice u8[]) {
    for idx usize in 0..slice.count {
        io.printf("%c", slice[idx])
    }
    io.printf("\n")

    for idx usize in 0..slice.count {
        slice[idx] = 'a' + unsafe_cast<u8>idx
    }

    for idx usize in 0..slice.count {
        io.printf("%c", slice[idx])
    }
    io.printf("\n")
}

main :: fn() i32 {
    let arr coll.Arr(<u8, 6>) = coll.arr_new(<u8, 6>)("hello")
    defer coll.arr_free(&arr);

    read(coll.arr_slice(arr));

    return 0
}
