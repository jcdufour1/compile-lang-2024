type io import = std.io

type Div struct {
    numerator i32
    denominator i32
}


type Token enum {
    div Div
    num i32
    string u8*
    character u8
}

fn main() i32 {
    let div Div = {.numerator = 34, .denominator = 97}
    io.printf("countof div = %zu\n", countof div)

    let token Token = .num(3)
    io.printf("countof token = %zu\n", countof token)

    return 0
}
