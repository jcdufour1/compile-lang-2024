
sub :: fn(<NumType>)(lhs NumType, rhs NumType) {
    let result NumType = lhs - rhs
    return result
}

add :: fn(<NumType, DummyType>)(lhs NumType, rhs NumType, dummy DummyType) {
    let result NumType = lhs + rhs
    return result
}

add_using_callback :: fn(lhs i32, rhs i32, add_fn fn()i32) i32 {
    return add_fn(<i32>)()
}

Token :: struct(<ItemType, PosType>) {
    item ItemType
    pos PosType
}

add_not_regular :: fn() i32 {
    return 7
}

main :: fn() i32 {
    let num1 i32 = sub(<i32, i64>)(7, 8)
    let num2 i32 = add(<i32>)(7, 8, 9)
    let token1 Token = {.item = 87, .pos = 32}
    let token2 Token(<i32, i32, i32>) = {.item = 88, .pos = 33}
    add_using_callback(8, 2, add_not_regular)
    return 0
}
