io :: import = std.io
rt :: import = std.runtime


get_zero :: fn() i32 {
    return 0
}

get_four :: fn() i32 {
    return 4
}

main :: fn() i32 {
    let string u8* = "hello"

    io.printf("%c\n", rt.buf_at(string, get_zero()))
    io.printf("%c\n", rt.buf_at(string, get_four()))

    io.printf("%c\n", rt.buf_at(string, 0))
    io.printf("%c\n", rt.buf_at(string, 4))

    io.printf("%c\n", rt.buf_at(string, 2*2))

    return 0
}
