
type io import = std.io

fn add_one_or_two_default(lhs ItemType, rhs io.Optional(<ItemType>) = .none, ItemType Type) ItemType {
    switch rhs {
        case .some(rhs_val):
            return lhs + rhs_val
        case .none:
            return lhs
    }
    return lhs
}

fn add_one_or_two_default_2(lhs io.Optional(<ItemType>), rhs io.Optional(<ItemType>) = .none, ItemType Type) ItemType {
    switch rhs {
        case .some(rhs_val):
            return lhs + rhs_val
        case .none:
            return lhs
    }
    return lhs
}

fn main() i32 {
    io.printf("test 1: got number %d\n", add_one_or_two_default_2(835, .some(11)))
    return 0
}

