io :: import = std.io
coll :: import = std.collections
util :: import = std.util

type Token struct(<NumT, StrT>) {
    num NumT
    string StrT
}

fn main() i32 {
    let arr coll.Arr(<Token(<i32, u8*>), 1000000>) = coll.arr_new(<Token(<i32, u8*>), 1000000>)()
    coll.arr_append(<Token(<i32, u8*>)>)(&arr, {9187, "hello"})
    io.printf("%s\n", coll.arr_first(<Token(<i32, u8*>)>)(arr).string);
    return 0
}
