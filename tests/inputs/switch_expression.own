io :: import = std.io


Bool :: enum {
    false
    true
}

is_true :: fn() Bool {
    return .true
}

is_false :: fn() Bool {
    return .false
}

main :: fn() i32 {
    num1 i32 := switch (is_true()) {
        case .true: break 2
        case .false: break 3
    }

    num2 i32 := switch (is_false()) {
        case .true: break 2
        case .false: break 3
    }

    io.printf("num1: %d\n", num1)
    io.printf("num2: %d\n", num2)

    return 0
}
