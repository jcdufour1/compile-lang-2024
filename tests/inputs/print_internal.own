io :: import = std.io
libc :: import = std.libc

main :: fn() i32 {
    num i32 := 2_100_005_009

    io.print_internal({"num = %%\n", []}, io.print_buf_file, io.stdout())
    io.print_internal({"num = %.\n", [{unsafe_cast<opaque*>&num, io.print_i32}]}, io.print_buf_file, io.stderr())
    io.print_internal({"65%% is not the same as %%%%, %%%%%. or %.%%%%\n", [{unsafe_cast<opaque*>&num, io.print_i32}, {unsafe_cast<opaque*>&num, io.print_i32}]}, io.print_buf_file, io.stderr())


    return 0
}

