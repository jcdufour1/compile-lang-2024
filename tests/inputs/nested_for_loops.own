io :: import = std.io

main :: fn() i32 {
    for num1 i32 in 0..7 {
        for num2 i32 in 0..num1 {
            io.printf("%d ", num2)
        }
        io.printf("\n")
    }

    for num1 i32 in 2..9 {
        for num2 i32 in num1..9 {
            io.printf("%d ", num2)
        }
        io.printf("\n")
    }

    return 0
}
