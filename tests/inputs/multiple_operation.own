fn bitwise_xor() {
    io.printf("bitwise xor:\n")
    {
        let num i32 = 0
        num = num ^ 0
        io.printf("%d\n", num)
    }

    {
        let num i32 = 0
        num = num ^ 1
        io.printf("%d\n", num)
    }

    {
        let num i32 = 1
        num = num ^ 1
        io.printf("%d\n", num)
    }

    {
        let num i16 = 2
        num = num ^ 1
        io.printf("%d\n", num)
    }

    {
        let num i32 = 2
        num = num ^ 2
        io.printf("%d\n", num)
    }

    io.printf("%d\n", 76 ^ 23)
}

fn bitwise_and() {
    io.printf("bitwise and:\n")
    {
        let num i32 = 0
        num = num & 0
        io.printf("%d\n", num)
    }

    {
        let num i32 = 0
        num = num & 1
        io.printf("%d\n", num)
    }

    {
        let num i32 = 1
        num = num & 1
        io.printf("%d\n", num)
    }

    {
        let num i32 = 2
        num = num & 1
        io.printf("%d\n", num)
    }

    {
        let num i32 = 2
        num = num & 2
        io.printf("%d\n", num)
    }

    io.printf("%d\n", 76 & 23)
}

fn bitwise_or() {
    io.printf("bitwise or:\n")
    {
        let num i32 = 0
        num = num | 0
        io.printf("%d\n", num)
    }

    {
        let num i32 = 0
        num = num | 1
        io.printf("%d\n", num)
    }

    {
        let num i32 = 1
        num = num | 1
        io.printf("%d\n", num)
    }

    {
        let num i32 = 2
        num = num | 1
        io.printf("%d\n", num)
    }

    {
        let num i32 = 2
        num = num | 2
        io.printf("%d\n", num)
    }

    io.printf("%d\n", 76 | 23)
}

fn bitwise_not() {
    io.printf("bitwise not:\n")
    {
        let num i32 = 0
        num = ~num
        io.printf("%x\n", num)
    }

    {
        let num i32 = 0
        num = ~1
        io.printf("%x\n", num)
    }

    {
        let num i16 = 1
        num = ~num
        io.printf("%x\n", unsafe_cast<i32>num)
    }

    {
        let num u32 = 0xFFFFFFFF
        num = ~num
        io.printf("%x\n", num)
    }
}

fn bitwise() {
    bitwise_xor()
    bitwise_and()
    bitwise_or()
    bitwise_not()
}

