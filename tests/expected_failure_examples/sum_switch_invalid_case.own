
extern("c") fn printf(str_to_print u8*, args any...) i32

type Token sum {
    num i32
    string u8*
}

type Thing sum {
    token Token
    num i32
}

fn token_print(token Token) {
    switch token {
        case Token.num(num): printf("token is number: %d\n", num)
        case Thing.token(token): printf("token is Thing.token")
    }
}

fn main() i32 {
    token_print({.num = 76, .string = "hello"})
    return 0
}

