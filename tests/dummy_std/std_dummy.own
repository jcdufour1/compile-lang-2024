io :: import = std.io

print_color :: fn(color Color) {
    io.printf("r: %u; g: %u; b: %u\n", color.r, color.g, color.b)
}

// test that user defined struct will work for extern("c") functions
print_color_extern :: extern("c") fn(color Color)

Color :: struct {
    r u32;
    g u32;
    b u32;
}

printf :: fn(string u8*) {
    io.printf("from module dummy one: ")
    io.printf("%s\n", string)
}


true :: fn() i32 {
    return 1
}

false :: fn() i32 {
    return 0
}

