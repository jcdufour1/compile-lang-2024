type io import = std.io


type Bool enum {
    false
    true
}

fn is_true() Bool {
    return .true
}

fn is_false() Bool {
    return .false
}

fn main() i32 {
    let num1 i32 = switch (is_true()) {
        case .true: break 2
        case .false: break 3
    }
    io.assert(0)

    let num2 i32 = switch (is_false()) {
        case .true: break 2
        case .false: break 3
    }
    io.assert(0)

    io.printf("num1: %d\n", num1)
    io.printf("num2: %d\n", num2)

    return 0
}
